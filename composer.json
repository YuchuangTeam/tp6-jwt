{
    "name": "cuarb/tp6-jwt",
    "description": "thinkphp6 jwt auth composer",
    "type": "library",
    "require": {
        "php": "^7.0 || ^8.0",
        "lcobucci/jwt": "4.1.5",
        "topthink/framework": "^6.0.0"
    },
    "license": "MIT",
    "authors": [
        {
            "name": "cuarb",
            "email": "info@cuarb.com"
        }
    ],
    "autoload": {
        "psr-4": {
            "cuarb\\jwt\\": "src"
        }
    },
    "extra": {
        "think": {
            "services": [
                "cuarb\\jwt\\JwtService"
            ],
            "config": {
                "jwt": "config/jwt.php"
            }
        }
    },
    "minimum-stability": "dev"
}
