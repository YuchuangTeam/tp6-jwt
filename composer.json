{
    "name": "cuarb/tp6-jwt",
    "description": "thinkphp jwt auth composer",
    "type": "library",
    "require": {
        "php": "^7.0 || ^8.0",
        "lcobucci/jwt": "4.1.5",
        "topthink/framework": "^6.0.*"
    },
    "license": "MIT",
    "authors": [
        {
            "name": "cuarb",
            "email": "info@cuarb.com"
        }
    ],
    "autoload": {
        "psr-4": {
            "cuarb\\jwt\\": "src"
        },
        "files": [
            "src/helper.php"
        ]
    },
    "extra": {
        "think": {
            "services": [
                "cuarb\\jwt\\Service"
            ],
            "config": {
                "jwt": "config/config.php"
            }
        }
    },
    "minimum-stability": "stable"
}
