{
    "name": "cuarb/tp-jwt-auth",
    "description": "thinkphp  jwt auth composer",
    "type": "library",
    "require": {
        "php": "^7.0 || ^8.0",
        "lcobucci/jwt": "4.1.*",
        "topthink/framework": "^5.1.10 || ^6.0.0"
    },
    "license": "MIT",
    "authors": [
        {
            "name": "cuarb",
            "email": "info@cuarb.com"
        }
    ],
    "autoload": {
        "psr-4": {
            "cuarb\\jwt\\": "src"
        },
        "files": [
            "src/helper.php"
        ]
    },
    "extra": {
        "think": {
            "services": [
                "cuarb\\jwt\\Service"
            ],
            "config": {
                "jwt": "config/config.php"
            }
        }
    },
    "minimum-stability": "dev"
}
